{
  "expo": {
    "name": "CocoWallet",
    "slug": "cocowallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#171C32",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#171C32",
      "androidSplashMode": "fade",
      "ios": {
        "loadingIndicator": true
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "dark",
      "backgroundColor": "#171C32",
      "bundleIdentifier": "com.cocowallet.cocowallet",
      "icon": "./assets/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-adaptive-foreground.png",
        "backgroundColor": "#171C32"
      },
      "softwareKeyboardLayoutMode": "pan",
      "allowBackup": true,
      "userInterfaceStyle": "dark",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "package": "com.cocowallet.cocowallet",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-system-ui",
        {
          "androidNavigationBar": {
            "backgroundColor": "#171C32"
          },
          "androidStatusBar": {
            "backgroundColor": "#171C32",
            "style": "light"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d919025a-0f0e-4ac6-923a-0f566ee4e792"
      }
    }
  }
}